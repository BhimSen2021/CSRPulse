@model IEnumerable<ProcessSetupModel>
<div class="card">
    <div class="card-body">
        <table id="basic-datatable" class="table table-hover dt-responsive nowrap w-100">
            <thead class="thead-light">
                <tr>
                    <th width="5%">#</th>
                    <th>@Html.DisplayNameFor(m => m.Skip)</th>
                    <th>@Html.DisplayNameFor(m => m.PrimaryRole)</th>
                    <th>@Html.DisplayNameFor(m => m.SecondaryRole)</th>
                    <th>@Html.DisplayNameFor(m => m.LevelName)</th>
                    <th>@Html.DisplayNameFor(m => m.Sequence)</th>
                </tr>
            </thead>
            <tbody>
                @if (Model.Count() > 0)
                {
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                <span>@(Model.ToList().IndexOf(item) +1)</span>
                            </td>

                            <td>@item.Skip</td>
                            <td>@item.PrimaryRole</td>
                            <td>@item.SecondaryRole</td>
                            <td>@item.LevelName</td>
                            <td>
                                @item.Sequence
                            </td>
                        </tr>

                    }
                }
                else
                {
                    <tr>
                        <td colspan="6">No Records Found</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>