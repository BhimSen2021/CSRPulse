@model CSRPulse.Model.CustomerSignIn
@{

    ViewData["Title"] = "Login";
    Layout = "~/Views/Shared/_LoginLayout.cshtml";

}

<h4 class="mt-0">Sign In</h4>
<p class="text-muted mb-4">Enter your Customer, User id and password to access account.</p>

<div id="div-form-container">
    @{ await Html.RenderPartialAsync("_Authenticate", Model);}
</div>


